{
  "name": "hotspot for hassio",
  "version": "1.0.0",
  "slug": "hassio-hotspot",
  "description": "Configure Hotspot",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/lxmicro/hassio-test",
  "advanced": true,
  "startup": "system",
  "boot": "auto",
  "host_network": true,
  "apparmor": false,
  "host_dbus": true,
  "full_access": true,
  "privileged": [
    "NET_ADMIN",
    "SYS_ADMIN",
    "SYS_RAWIO",
    "SYS_TIME",
    "SYS_NICE"
  ],
  "devices": [
    "/dev/mem:/dev/mem:rwm"
  ],
  "map": [
    "config:rw",
    "ssl:rw",
    "addons:rw",
    "share:rw",
    "backup:rw"
  ],
  "options": {
    "hostapd": [
      {
        "name": "HassioPortal",
        "passphrase": "1234567890",
        "channel": "7",
        "hostad_interface": "wlan0",
        "hostap_ip": "10.0.0.1",
        "country_code": "US",
        "hw_mode": "g"
      }
    ]
  },
  "schema": {
    "hostapd": [
      {
      "name": "str",
      "passphrase": "str",
      "channel": "int",
      "hostad_interface": "str",
      "hostap_ip": "str",
      "country_code": "str",
      "hw_mode": "str"
      }
    ]
  }
}
{
  "name": "Hassio Test",
  "version": "1.1.3",
  "slug": "hassio-test",
  "description": "test for hassio",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/lxmicro/hassio-test",
  "startup": "system",
  "boot": "auto",
  "host_network": true,
  "apparmor": true,
  "host_dbus": true,
  "full_access": true,
  "privileged": [
    "NET_ADMIN",
    "SYS_ADMIN",
    "SYS_RAWIO",
    "SYS_TIME",
    "SYS_NICE"
  ],
  "devices": [
    "/dev/mem:/dev/mem:rwm"
  ],
  "map": [
    "config:rw",
    "ssl:rw",
    "addons:rw",
    "share:rw",
    "backup:rw"
  ],
  "options": {
    "mqtt_server": "",
	"mqtt_topic": ""
  },
  "schema": {
    "mqtt_server": "str",
	"mqtt_topic": "str"
  }
} 
